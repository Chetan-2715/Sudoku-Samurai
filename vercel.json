{
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/index.py"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "buildCommand": "cd frontend && yarn && yarn build",
  "outputDirectory": "frontend/build",
  "framework": "create-react-app"
}